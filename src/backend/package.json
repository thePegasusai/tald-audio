{
  "name": "tald-unia-backend",
  "version": "1.0.0",
  "description": "TALD UNIA Audio System Backend Service - Enterprise-grade audio processing system",
  "author": "TALD UNIA Team",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "security:audit": "npm audit",
    "docker:build": "docker build -t tald-unia-backend .",
    "docker:run": "docker run -p 3000:3000 tald-unia-backend"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/helmet": "^8.0.0",
    "@nestjs/throttle": "^4.0.0",
    "@nestjs/platform-ws": "^10.0.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nestjs/axios": "^3.0.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "@tensorflow/tfjs-node-gpu": "^2.13.0",
    "@opentelemetry/api": "^1.4.0",
    "bull": "^4.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "fft.js": "^4.0.3",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "joi": "^17.9.0",
    "newrelic": "^11.0.0",
    "node-cuda": "^0.7.0",
    "numeric": "^1.2.6",
    "passport-jwt": "^4.0.0",
    "pg": "^8.11.0",
    "pino": "^8.0.0",
    "prom-client": "^14.0.0",
    "redis": "^7.2.0",
    "rxjs": "^7.8.0",
    "socket.io": "^4.7.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "wasm-simd": "^1.0.0",
    "winston": "^3.10.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@fastify/rate-limit": "^8.0.0",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "performance-now": "^2.1.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}